  `cedula` varchar(50) COLLATE utf8_spanish2_ci NOT NULL,
  `nina` int(11) NOT NULL,
  `diag_sit` int(11) NOT NULL,
  `obj_gene` int(11) NOT NULL,
  `area` int(11) NOT NULL,
  PRIMARY KEY (`id_pgf`),
